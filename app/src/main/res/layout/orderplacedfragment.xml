<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView133"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/poppins"
            android:gravity="center"
            android:text="There is nothing in your bag. Let's add some items."
            android:textColor="#3F4E6E"
            android:textSize="13sp"
            app:layout_constraintBottom_toTopOf="@+id/textView134"
            app:layout_constraintEnd_toEndOf="@+id/imageView46"
            app:layout_constraintStart_toStartOf="@+id/imageView46"
            app:layout_constraintTop_toBottomOf="@+id/imageView46" />

        <TextView
            android:id="@+id/textView134"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="32dp"
            android:layout_marginBottom="26dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="We received your order and will begin processing it soon. Please check your email and message for order related details."
            android:textAlignment="center"
            android:textColor="#262E3F"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/button13"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView133" />

        <Button
            android:id="@+id/button13"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:layout_marginStart="26dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="29dp"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:layout_marginBottom="209dp"
            android:background="@drawable/round_for_search"
            android:fontFamily="@font/poppins_semibold"
            android:text="Shop More"
            android:textSize="15sp"
            app:backgroundTint="#EDA537"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/textView134"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintStart_toStartOf="@+id/textView134"
            app:layout_constraintTop_toBottomOf="@+id/textView134" />

        <ImageView
            android:id="@+id/imageView46"
            android:layout_width="234dp"
            android:layout_height="227dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="133dp"
            android:layout_marginEnd="64dp"
            android:layout_marginBottom="19dp"
            android:src="@drawable/congo"
            app:layout_constraintBottom_toTopOf="@+id/textView133"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>