<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_20sdp"
        android:layout_marginTop="16dp"
        android:background="@drawable/round_corner"
        android:backgroundTint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/imageView89"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="20dp"
            android:backgroundTint="#D5EFF3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/circle_gray3" />

        <TextView
            android:id="@+id/textView254"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:fontFamily="@font/poppins_semibold"
            android:text="Hemoglobin"
            android:textColor="#090909"
            android:textStyle="bold"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/imageView89"
            app:layout_constraintTop_toTopOf="@+id/imageView89" />

        <TextView
            android:id="@+id/textView255"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="Normal: 14.0-18.0"
            android:textColor="#3F4E6E"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView89"
            app:layout_constraintStart_toStartOf="@+id/textView254"
            app:layout_constraintTop_toBottomOf="@+id/textView254" />

        <TextView
            android:id="@+id/textView256"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_semibold"
            android:text="21.9 g/dL"
            android:textColor="#E10C0C"
            app:layout_constraintBottom_toBottomOf="@+id/textView254"
            app:layout_constraintTop_toTopOf="@+id/textView254"
            tools:layout_editor_absoluteX="269dp" />

        <TextView
            android:id="@+id/textView257"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="High (+ 3.9)"
            android:textColor="#62708E"
            app:layout_constraintBottom_toBottomOf="@+id/textView255"
            app:layout_constraintStart_toStartOf="@+id/textView256"
            app:layout_constraintTop_toBottomOf="@+id/textView256" />

        <ImageView
            android:id="@+id/imageView92"
            android:layout_width="29dp"
            android:layout_height="19dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="5dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView256"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.8"
            app:layout_constraintStart_toEndOf="@+id/textView256"
            app:layout_constraintTop_toTopOf="@+id/textView256"
            app:srcCompat="@drawable/graphs_icon" />

        <TextView
            android:id="@+id/textView260"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:justificationMode="inter_word"
            android:text="The process of determining which disease or condition explains a person's symptom and signs. It is most often referred to as diagnosis with the medical context being implicit."
            android:textAlignment="center"
            android:textColor="#3F4E6E"
            android:textDirection="ltr"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView92"
            app:layout_constraintStart_toStartOf="@+id/imageView89"
            app:layout_constraintTop_toBottomOf="@+id/textView255" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>